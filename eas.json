{
  "cli": {
    "version": ">= 2.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "android": { "buildType": "apk" }
    },
    "staging": {
      "distribution": "internal",
      "ios": { "image": "latest" }
    },
    "production": {
      "autoIncrement": true,
      "ios": { "image": "latest" }
    }
  }
}
