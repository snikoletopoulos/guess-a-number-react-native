{
  "expo": {
    "name": "Guess a Number",
    "slug": "guess-a-number",
    "description": "A project to get familiar with React Native",
    "githubUrl": "https://github.com/snikoletopoulos/guess-a-number-react-native",
    "version": "1.0.0",
    "orientation": "default",
    "owner": "snikoletopoulos",
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nikero.guess"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nikero.guess"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "backgroundColor": "#ffffff",
          "imageWidth": 400
        }
      ]
    ],
    "extra": { "eas": { "projectId": "9071a306-6134-4e5e-bd1b-0525b32cd125" } }
  }
}
